<!DOCTYPE html>
<html>
  <head>
    <title><%= generate_page_title(yield(:title)) %></title>
    <%= render 'layouts/setup' %>
  </head>
  <body>
    <%= render 'layouts/header' %>
    <div class="container">
      <%= render 'layouts/flash_messages' %> 
      <%= yield %>
      <%= render 'layouts/footer' %>
      <%= debug(params) if Rails.env.development? %>
    </div>
  </body>
</html>
